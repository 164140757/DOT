{   
    "lr_sigma": 1e0,
    "lr_sigma_final": 5e-4,
    "lr_sigma_decay_steps": 1e5,
    "lr_sigma_delay_steps": 0,
    "lr_sigma_delay_mult": 0.01,
    "lr_sh": 5e-1,
    "lr_sh_final": 5e-5,
    "lr_sh_decay_steps": 1e5,
    "lr_sh_delay_steps": 0,
    "lr_sh_delay_mult": 0.01,
    "prune_max":6.0,

    "use_sparsity_loss":true,
    "init_weight_sparsity_loss":1e-2,
    "lr_sparsity_loss":1e-4,

    "rms_beta":0.95,

    "hessian_mse":5e-4,
